* ------------- REVENUE --------------
ECO_G_YCRAG_scenarios(scenarios,Y,C,IR,IA,G,FFF,TECH_TYPE,'REVENUE','TOTAL_REVENUE','Mmoney')$IAGK_HASORPOT(Y,IA,G)=
SUM(SUBCATEGORY, ECO_G_YCRAG_scenarios(scenarios,Y,C,IR,IA,G,FFF,TECH_TYPE,'INCOME',SUBCATEGORY,'Mmoney')-ECO_G_YCRAG_scenarios(scenarios,Y,C,IR,IA,G,FFF,TECH_TYPE,'COSTS',SUBCATEGORY,'Mmoney')
)
;

ECO_G_YCRAG_scenarios(scenarios,Y,C,IR,IA,G,FFF,TECH_TYPE,'REVENUE','ENERGY_SPECIFIC_REVENUE','Money_per_MWh')$(IAGK_HASORPOT(Y,IA,G) AND SUM(COMMODITY,PRO_YCRAGF_scenarios(scenarios,Y,C,IR,IA,G,FFF,COMMODITY,TECH_TYPE,'TWh') > 0)) =
ECO_G_YCRAG_scenarios(scenarios,Y,C,IR,IA,G,FFF,TECH_TYPE,'REVENUE','TOTAL_REVENUE','Mmoney')/SUM(COMMODITY,PRO_YCRAGF_scenarios(scenarios,Y,C,IR,IA,G,FFF,COMMODITY,TECH_TYPE,'TWh'))
;
